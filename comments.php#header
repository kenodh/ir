<script type="text/javascript">
	function viewMonth(year,month) {
		document.frmComment.year.value = year;
		document.frmComment.month.value = month;
		document.frmComment.submit();
	}

	function writeComment() {
		document.frmComment.year.value = 0;
		document.frmComment.month.value = 0;
		document.frmComment.submit();
	}
	
	function onSubmitClick() {
		document.frmWriteComment.year.value = 0;
		document.frmWriteComment.month.value = 0;
		document.frmWriteComment.cmdSubmit.disabled = true;
		document.frmWriteComment.submit();
	}
</script>

	<form name="frmComment" method="post" class="nodisplay">
		<input type="hidden" name="year" value="0">
		<input type="hidden" name="month" value="0">
	</form>

